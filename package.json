{
  "name": "scanwarp",
  "version": "0.1.0",
  "private": true,
  "description": "Your AI writes your code. ScanWarp keeps it running.",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
